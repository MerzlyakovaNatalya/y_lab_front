{
  "name": "react-webinar-3",
  "version": "1.0.0",
  "description": "Example project for webinar",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "vite serve",
    "dev": "cross-env NODE_ENV=development IS_SERVER=true node ./server/server.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir ../dist/client --ssrManifest",
    "build:server": "vite build --outDir ../dist/server --ssr entry-server.tsx",
    "prod": "cross-env NODE_ENV=production node ./server/server.js",
    "test": "jest",
    "playwright": "npx playwright test"
  },
